<config>
  <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
    <pseudowire-class>
        <name>{{ pseudowie["name"] }}</name>
        <encapsulation xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-l2vpn">{{ pseudowie["encapsulation"] }}</encapsulation>
    </pseudowire-class>
    <interface>
      <GigabitEthernet>
        <name>{{ pseudowie["interface"] }}</name>
        <xconnect xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-l2vpn">
          <address>{{ pseudowie["ip-address"] }}</address>
          <vcid>{{ pseudowie["vcid"] }}</vcid>
          <encapsulation>{{ pseudowie["encapsulation"] }}</encapsulation>
          <pw-class>{{ pseudowie["name"] }}</pw-class>
        </xconnect>
      </GigabitEthernet>
    </interface>      
  </native>
</config>