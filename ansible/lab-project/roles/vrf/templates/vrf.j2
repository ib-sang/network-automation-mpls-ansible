#jinja2: lstrip_blocks: "true"
<config>
  <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
    {% if vrfs is defined%} 
      {% for vrf in vrfs %}
        <vrf xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-vrf">
          <name>{{ vrf["name"] }}</name>
          <create/>
          <rd>{{ vrf["rd"] }}</rd>
          <route-target>
            <direction>export</direction>
            <target>{{ vrf["route-export"] }}</target>
          </route-target>  
          <route-target>
            <direction>import</direction>
            <target>{{ vrf["route-import"] }}</target>
          </route-target>        
        </vrf>
        {% endfor %}
    {% endif %}    
  </native>
</config>
